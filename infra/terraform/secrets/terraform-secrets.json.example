{
  "github_owner": "your-org-or-username",
  "github_token": "ghp_your_github_token_here",
  "repository_secrets": {
    "UNITY_LICENSE": "your-unity-license-content-here",
    "UNITY_EMAIL": "your-unity-email@example.com",
    "UNITY_PASSWORD": "your-unity-password",
    "UNITY_SERIAL": "your-unity-serial-if-applicable",
    "ANDROID_KEYSTORE_BASE64": "base64-encoded-keystore-content",
    "ANDROID_KEYSTORE_PASS": "your-keystore-password",
    "ANDROID_KEY_ALIAS": "your-key-alias",
    "ANDROID_KEY_PASS": "your-key-password",
    "IOS_CERTIFICATE_BASE64": "base64-encoded-certificate",
    "IOS_PROVISION_BASE64": "base64-encoded-provisioning-profile",
    "IOS_CERTIFICATE_PASSWORD": "your-certificate-password"
  },
  "repository_variables": {
    "UNITY_VERSION": "2022.3.x",
    "DOTNET_VERSION": "8.0.x",
    "NODEJS_VERSION": "20.x",
    "BUILD_CONFIGURATION": "Release",
    "ENABLE_CODE_COVERAGE": "true",
    "ENABLE_UNITY_CACHE": "true",
    "BUILD_ANDROID": "true",
    "BUILD_IOS": "true",
    "BUILD_WEBGL": "false",
    "SPEC_KIT_ENABLED": "true"
  }
}
